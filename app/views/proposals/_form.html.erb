<%= form_for(@proposal) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :address %>
  <%= f.text_area :address, rows: 6 %>
 
  <%= f.label :description %>
  <%= f.text_area :description, rows: 6 %>
  
  <%= f.submit "Create proposal", class: "btn btn-large btn-primary" %>

<% end %>
