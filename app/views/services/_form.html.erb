<%= form_for(@service) do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :description, rows: 6 %>

    <%= f.label :price %>
    <%= f.text_field :price %>

    <%= f.submit "Create services", class: "btn btn-large btn-primary" %>

<% end %>
